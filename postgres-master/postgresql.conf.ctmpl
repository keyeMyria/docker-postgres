archive_command = 'pg_archive_command.sh %p %f'
archive_mode = 'on'
autovacuum = 'on'
data_directory = '/var/lib/postgresql/data'
datestyle = 'iso, mdy'
default_statistics_target = '300'
default_text_search_config = 'pg_catalog.english'
effective_cache_size = '{{key_or_default "docker-server-configs/services/postgres-master.json/effective_cache_size" "4GB"}}'
lc_messages = 'en_US.UTF-8'
lc_monetary = 'en_US.UTF-8'
lc_numeric = 'en_US.UTF-8'
lc_time = 'en_US.UTF-8'
listen_addresses = '*'
log_line_prefix = '%m %u@%d %p %r '
log_lock_waits = 'on'
log_min_duration_statement = '1000'
maintenance_work_mem = '64MB'
max_connections = '80'
max_prepared_transactions = '25'
max_wal_senders = '3'
pg_stat_statements.max = '1000'
pg_stat_statements.save = 'off'
pg_stat_statements.track = 'top'
pg_stat_statements.track_utility = 'off'
shared_buffers = '{{key_or_default "docker-server-configs/services/postgres-master.json/shared_buffers" "128MB"}}'
shared_preload_libraries = 'pg_stat_statements,pg_amqp'
temp_buffers = '8MB'
track_activities = 'on'
track_counts = 'on'
wal_buffers = '16MB'
wal_keep_segments = '128'
wal_level = 'hot_standby'
wal_sync_method = 'fdatasync'
work_mem = '64MB'
